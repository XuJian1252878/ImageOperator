<?xml version="1.0" encoding="utf-8"?>
<com.example.multi_image_selector.view.SquareFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#343535"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- android:drawablePadding The padding between the drawables and the text. -->
    <!-- android:drawableTop The drawable to be drawn above the text. -->
    <TextView
        android:drawableTop="@drawable/mis_asy"
        android:drawablePadding="10dp"
        android:layout_gravity="center"
        android:text="@string/mis_tip_take_photo"
        android:textSize="14sp"
        android:textColor="#CDCECE"
        android:gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</com.example.multi_image_selector.view.SquareFrameLayout>
